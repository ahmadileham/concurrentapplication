<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="concurrentapplication.InputPageController">
    <stylesheets>
        <URL value="@inputpage.css" />
    </stylesheets>
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #EEF7FF;">
         <children>
            <TextField fx:id="filePathField" editable="false" layoutX="184.0" layoutY="212.0" promptText="Selected File" />
            <Button layoutX="347.0" layoutY="212.0" mnemonicParsing="false" onAction="#browseFile" text="Browse" />
            <Label layoutX="235.0" layoutY="74.0" text="CPP Infinty">
               <font>
                  <Font name="Franklin Gothic Heavy" size="25.0" />
               </font>
            </Label>
            <Label layoutX="184.0" layoutY="183.0" text="Bag of Words" />
            <Button fx:id="submitFileBtn" layoutX="410.0" layoutY="212.0" mnemonicParsing="false" onAction="#submitFile" text="Submit" />
         </children>
      </Pane>
   </children>
</AnchorPane>
